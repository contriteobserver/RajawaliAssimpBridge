            <diffuse>
              <texture texture="ripples_png-sampler" texcoord="UVMap"/>
              <color sid="diffuse">0.75 0.5 0 1</color>
            </diffuse>
            <index_of_refraction>
              <float sid="ior">1.45</float>
            </index_of_refraction>
          </lambert>
          <extra>
