        </newparam>
        <newparam sid="gridnormals_png-surface">
          <surface type="2D">
            <init_from>gridnormals_png</init_from>
          </surface>
        </newparam>
        <newparam sid="gridnormals_png-sampler">
          <sampler2D>
            <source>gridnormals_png-surface</source>
          </sampler2D>
        </newparam>
        <technique sid="common">
          <lambert>
            <emission>
              <color sid="emission">0 0 0 1</color>
            </emission>
            <diffuse>
              <texture texture="ripples_png-sampler" texcoord="UVMap"/>
              <color sid="diffuse">0 0.75 0.75 1</color>
            </diffuse>
            <index_of_refraction>
              <float sid="ior">1.45</float>
            </index_of_refraction>
          </lambert>
          <extra>
            <technique>
              <bump>
                  <texture texture="gridnormals_png-sampler" texcoord="UVMap"/>
              </bump>
            </technique>
          </extra>
        </technique>
      </profile_COMMON>
    </effect>
    <effect id="Green-effect">
      <profile_COMMON>
        <newparam sid="ripples_png-surface">
          <surface type="2D">
            <init_from>ripples_png</init_from>
          </surface>
        </newparam>
        <newparam sid="ripples_png-sampler">
